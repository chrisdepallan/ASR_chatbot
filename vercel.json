{
    "version": 2,
    "builds": [
      {
        "src": "api/main.py",
        "use": "@vercel/python",
        "config": { "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/api/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "api/main.py"
      }
    ]
  }